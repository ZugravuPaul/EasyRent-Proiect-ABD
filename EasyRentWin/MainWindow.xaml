<Window x:Class="EasyRentWin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EasyRentWin"
        mc:Ignorable="d"
        Title="MainWindow" Height="850" Width="1200">
    <Grid>
        
        <Button x:Name="btnSave" Content="Save" HorizontalAlignment="Left" Height="44" Margin="335,403,0,0" VerticalAlignment="Top" Width="559" Click="btnSave_Click"/>
        <TextBox x:Name="txtUsername" HorizontalAlignment="Left" Height="32" Margin="339,41,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="555" />
        <TextBox x:Name="txtEmail" HorizontalAlignment="Left" Height="33" Margin="339,159,0,0" TextWrapping="Wrap" Text="Email" VerticalAlignment="Top" Width="555"/>
        <TextBox x:Name="txtPhone" HorizontalAlignment="Left" Height="33" Margin="339,231,0,0" TextWrapping="Wrap" Text="Phone" VerticalAlignment="Top" Width="555"/>
        <PasswordBox x:Name="passPassword" HorizontalAlignment="Left" Margin="339,108,0,0" VerticalAlignment="Top" Width="555" Height="26"/>

    </Grid>
</Window>
